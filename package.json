    "url": "https://github.com/benstokoe/isomorphic-alt-hello-world/issues"
  },
  "homepage": "https://github.com/benstokoe/isomorphic-alt-hello-world#readme",
  "dependencies": {
    "babel": "^5.2.17",
    "express": "^4.12.3",
    "jade": "^1.9.2",
    "node-jsx": "^0.13.3",
    "react": "^0.13.2"
  }
