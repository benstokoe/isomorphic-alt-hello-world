    "babelify": "^6.0.2",
    "browserify": "^9.0.8",
    "envify": "^3.4.0",
    "reactify": "^1.1.0",
    "watchify": "^3.1.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/benstokoe/isomorphic-alt-hello-world.git"
