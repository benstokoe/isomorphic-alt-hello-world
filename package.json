{
  "name": "isomorphic-alt-hello-world",
  "version": "0.0.1",
  "description": "Test isomorphic app using alt",
  "main": "js/app.js",
  "scripts": {
    "start": "watchify -o dist/bundle.js -v -d js/app.js",
    "build": "browserify . -t [envify --NODE_ENV production] | uglifyjs -cm > dist/bundle.min.js",
